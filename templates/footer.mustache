{{!
    Page footer.
}}
<footer id="page-footer wb-info" class="py-3 bg-dark text-light">
    <div class="container">
	
        <div id="course-footer">{{{ output.course_footer }}}</div>

        {{# output.page_doc_link }}
            <p class="helplink">{{{ output.page_doc_link }}}</p>
        {{/ output.page_doc_link }}

        {{{ output.login_info }}}
		
        <div class="tool_usertours-resettourcontainer"></div>
        
        <nav class="nav navbar-nav d-md-none" aria-label="{{#str}}custommenu, admin{{/str}}">
            {{# output.custom_menu_flat }}
                <ul class="list-unstyled pt-3">
                    {{> theme_boost/custom_menu_footer }}
                </ul>
            {{/ output.custom_menu_flat }}
        </nav>
        {{{ output.standard_footer_html }}}
        {{{ output.standard_end_of_body_html }}}
		
		<section class="col-sm-3 footer-about">
		<h3><span>{{# str }} about, theme_wettheme{{/ str }}</span></h3>
		<ul class = "list-unstyled">
		<li><a href="{{# str }} department_link, theme_wettheme{{/ str }}">{{# str }} department, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} vision_link, theme_wettheme{{/ str }}">{{# str }} vision, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} org_chart_link, theme_wettheme{{/ str }}">{{# str }} org_chart, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} deputies_link, theme_wetheme{{/ str }}">{{# str }} deputies, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} senior_management_link, theme_wettheme{{/ str }}">{{# str }} senior_management, theme_wettheme {{/ str }}</a></li>
		</ul>
		</section>
		
		<section class="col-sm-3 footer-contact">
		<h3><span>{{# str }} contact, theme_wettheme{{/ str }}</span></h3>
		<ul class = "list-unstyled">
		<li><a href="{{# str }} comments_link, theme_wettheme{{/ str }}">{{# str }} comments, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} geds_link, theme_wettheme{{/ str }}">{{# str }} geds, theme_wettheme {{/ str }}</a></li>
		</ul>
		</section>
		
		<section class="col-sm-3 footer-news">
		<h3><span>{{# str }} news, theme_wettheme{{/ str }}</span></h3>
		<ul class = "list-unstyled">
		<li><a href="{{# str }} news_releases_link, theme_wettheme{{/ str }}">{{# str }} news_releases, theme_wettheme {{/ str }}</a></li>
		
		</ul>
		</section>
		
		<section class="col-sm-3 footer-stay-connected">
		<h3><span>{{# str }} stay_connected, theme_wettheme{{/ str }}</span></h3>
		<ul class = "list-unstyled">
		<li><a href="{{# str }} social_media_link, theme_wettheme{{/ str }}">{{# str }} social_media, theme_wettheme {{/ str }}</a></li>
		<li><a href="{{# str }} rss_feeds_link, theme_wettheme{{/ str }}">{{# str }} rss_feeds, theme_wettheme {{/ str }}</a></li>
		</ul>
		</section>
		
		<div class="col-md-12 footer-terms">
		<ul class="list-inline">
		<li><a href="{{# str }} terms_conditions_link, theme_wettheme{{/ str }}">{{# str }} terms_conditions, theme_wettheme {{/ str }}</a>
		<a href="{{# str }} transparency_link, theme_wettheme{{/ str }}">{{# str }} transparency, theme_wettheme {{/ str }}</a>
		</ul>
		</div>
		
		<div class="col-md-2 col-xs-6 text-right canada-logo">
		<img src="{{{canadalogo}}}" alt="Canada" width="168" height="50">
		</div>

    </div>
</footer>
